  "scripts": {
    "postinstall": "yarn migrate:up",
    "build": "webpack --config webpack.production.config.js",
    "start": "node index.js",
    "debug": "node --inspect index.js",
    "watch": "NODE_ENV=development concurrently --kill-others --raw \"nodemon index.js --config nodemon.json\"",
    "test": "yarn test:prepare && yarn test:server && yarn test:client",
    "test:cover": "yarn test:prepare && yarn test:server:cover && yarn test:client:cover",
    "test:prepare": "NODE_ENV=test yarn migrate:up && yarn build",
    "test:server": "NODE_ENV=test JWT_SECRET=shh USER_AUDITOR=user_auditor mocha --exit --timeout 2500 --recursive ./test/api",
    "test:client": "NODE_ENV=test mocha --exit --require @babel/register --recursive ./test/frontend",
    "test:server:cover": "NODE_ENV=test JWT_SECRET=shh USER_AUDITOR=user_auditor nyc --report-dir ./coverage/server mocha --exit --timeout 2500 --recursive ./test/api",
    "test:client:cover": "NODE_ENV=test nyc --report-dir ./coverage/client mocha --exit --require @babel/register --recursive ./test/frontend",
    "test:client:file": "mocha --exit --require @babel/register test/frontend/support/_bootstrap.js",
    "test:server:file": "NODE_ENV=test JWT_SECRET=shh USER_AUDITOR=user_auditor mocha --exit --timeout 2500 test/api/bootstrap.test.js",
    "test:server:watch": "watch 'yarn test:server' ./api ./test/api",
    "test:client:watch": "watch 'yarn test:client' ./frontend ./test/frontend",
    "lint": "eslint ./api ./config ./frontend ./scripts ./services --ext js,jsx",
    "migrate:create": "node migrate.js create",
    "migrate:up": "node migrate.js up || true",
    "migrate:down": "node migrate.js down",
    "migrate:test": "node migrate.js up --dry-run",
    "migrate:reset": "node migrate.js reset",
    "nightly-builds": "node ./scripts/nightly-builds.js",
    "export:sites": "node ./scripts/exportSitesAsCsv.js",
    "analyze-webpack": "webpack-bundle-analyzer -h 0.0.0.0 public/stats.json",
    "serve-coverage": "serve -l 8080 ./coverage",
    "update-local-config": "node ./scripts/update-local-config.js",
    "create-dev-data": "NODE_ENV=development node ./scripts/create-dev-data.js",
    "update-proxy-db": "node ./scripts/proxy-edge.js",
    "timeout-builds": "node ./scripts/timeout-builds.js",
    "remove-inactive-federalist-users": "node ./scripts/remove-inactive-federalist-users.js",
    "remove-federalist-users-org-members-not-users":  "node ./scripts/remove-federalist-users-org-members-not-users.js",
    "archive-build-logs": "node ./scripts/archive-build-logs.js",
    "archive-build-logs-daily": "node ./scripts/archive-build-logs-daily.js",
    "migrate-build-logs": "node ./scripts/migrate-build-logs.js"
  },
